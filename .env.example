# Environment variables example for ObjDet
# Copy this file to .env and fill in your values
# DO NOT commit .env files to version control

# =============================================================================
# MLflow Configuration
# =============================================================================
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=objdet
# MLFLOW_TRACKING_USERNAME=
# MLFLOW_TRACKING_PASSWORD=

# =============================================================================
# RabbitMQ Configuration
# =============================================================================
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=objdet
RABBITMQ_PASS=objdet_dev
CELERY_BROKER_URL=amqp://${RABBITMQ_USER}:${RABBITMQ_PASS}@${RABBITMQ_HOST}:${RABBITMQ_PORT}//
CELERY_RESULT_BACKEND=rpc://

# =============================================================================
# Application Configuration
# =============================================================================
OBJDET_LOG_LEVEL=INFO
OBJDET_LOG_FORMAT=rich  # "rich" or "json"

# =============================================================================
# GPU Configuration
# =============================================================================
CUDA_VISIBLE_DEVICES=0

# =============================================================================
# Serving Configuration
# =============================================================================
OBJDET_SERVE_HOST=0.0.0.0
OBJDET_SERVE_PORT=8000
OBJDET_SERVE_WORKERS=4
